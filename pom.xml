<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.xwiki.contrib</groupId>
    <artifactId>parent-platform</artifactId>
    <version>8.4-11</version>
  </parent>
  <groupId>org.xwiki.contrib</groupId>
  <artifactId>application-polls</artifactId>
  <version>1.0.1</version>
  <name>Polls Application</name>
  <packaging>xar</packaging>
  <description>An application allowing the creation of Polls in XWiki</description>
  <scm>
    <connection>scm:git:git://github.com/xwiki-contrib/application-polls.git</connection>
    <developerConnection>scm:git:git@github.com:xwiki-contrib/application-polls.git</developerConnection>
    <url>https://github.com/xwiki-contrib/application-polls.git</url>
    <tag>application-polls-1.0.1</tag>
  </scm>
  <developers>
    <developer>
      <id>sdumitriu</id>
      <name>Sergiu Dumitriu</name>
    </developer>
    <developer>
      <id>marta</id>
      <name>Marta Girdea</name>
    </developer>
    <developer>
      <id>vmassol</id>
      <name>Vincent Massol</name>
    </developer>
  </developers>
  <properties>
    <!-- Don't run CLIRR here since there's no Java code. -->
    <xwiki.clirr.skip>true</xwiki.clirr.skip>

    <!-- Old names of this module used for retro compatibility when resolving dependencies of old extensions -->
    <xwiki.extension.features>org.xwiki.contrib:xwiki-application-polls</xwiki.extension.features>

    <xwiki.release.jira.skip>false</xwiki.release.jira.skip>
    <xwiki.issueManagement.jira.id>XPOLLAPP</xwiki.issueManagement.jira.id>
  </properties>
</project>
